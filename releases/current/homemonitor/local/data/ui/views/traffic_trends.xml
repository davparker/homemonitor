<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Traffic Trends</label>
  <row>
    <chart>
      <searchName>Realtime IN Bound TCP Traffic (30 Minute)</searchName>
      <title>Real Time TCP In bound traffic (Last 30 Minutes)</title>
      <option name="charting.chart">radialGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.rangeValues">[0,1000,4000,10000]</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.gaugeColors">[0x84e900,0xffe800,0xbf3030]</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
    <chart>
      <searchName>Realtime OUT Bound TCP Traffic (30 Minute)</searchName>
      <title>Real Time TCP Out bound traffic (Last 30 Minutes)</title>
      <option name="charting.chart">radialGauge</option>
      <option name="charting.chart.rangeValues">[0,1000,4000,10000]</option>
      <option name="charting.gaugeColors">[0x84e900,0xffe800,0xbf3030]</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Real Time (30 Minute Window) In vs. Out bound traffic</searchName>
      <title>Real Time (60 Minute Window) In vs. Out bound traffic</title>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Connections</option>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Real Time Accept vs. Block Last 60 Minute</searchName>
      <title>Real Time Accept vs. Block Last 60 Minute</title>
      <option name="charting.chart">area</option>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>In vs. Out bound traffic broken down by transport (Last 60 minutes)</searchName>
      <title>In vs. Out bound traffic broken down by transport (Last 60 minutes)</title>
      <option name="charting.axisTitleY.text">Connections</option>
      <option name="charting.chart">area</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.primaryAxisTitle.text">Time</option>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>Chatty Clients Last 15 minutes</searchName>
      <title>Chatty Clients Last 60 minutes</title>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Connections</option>
      <option name="charting.chart">area</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
  </row>
  <row>
    <chart>
      <searchName>In vs. Out bound traffic broken down by Country (Last 60 minutes)</searchName>
      <title>In vs. Out bound traffic broken down by Country (Last 60 minutes)</title>
      <option name="charting.chart">area</option>
    </chart>
  </row>
</dashboard>
