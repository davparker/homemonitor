<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Router Configurations</label>
  <row>
    <chart>
      <searchName>Number of Configuration Changes to Router Last 30 Days</searchName>
      <title>Number of Configuration Changes to Router Last 30 Days</title>
      <option name="charting.chart">fillerGauge</option>
    </chart>
    <chart>
      <searchName>Number of Configuration Changes to Router Last 7 Days</searchName>
      <title>Number of Configuration Changes to Router Last 7 Days</title>
      <option name="charting.chart">fillerGauge</option>
    </chart>
    <chart>
      <searchName>Number of Configuration Changes to Router</searchName>
      <title>Number of Configuration Changes to Router (Real time 30 minute window)</title>
      <option name="charting.chart">fillerGauge</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">minimal</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <table>
      <searchName>Configuration Changes to Router by user Last 30 Days</searchName>
      <title>Configuration Changes to Router by user Last 30 Days</title>
    </table>
    <table>
      <searchName>Configuration Changes to Router by user Last 7 Days</searchName>
      <title>Configuration Changes to Router by user Last 7 Days</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
    <table>
      <searchName>Configuration Changes to Router</searchName>
      <title>Real Time (last 30 minutes) Configuration Changes to Router</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
      <option name="maxLines">10</option>
      <option name="segmentation">full</option>
      <option name="softWrap">True</option>
    </table>
  </row>
  <row>
    <chart>
      <searchName>Connection States Last 24 Hours</searchName>
      <title>Connection States Last 24 Hours</title>
      <option name="charting.chart">pie</option>
    </chart>
    <chart>
      <searchName>Non Connection based router messages</searchName>
      <title>Non Connection based router messages</title>
      <option name="charting.axisTitleX.text">Connection State</option>
      <option name="charting.axisTitleY.text">Attempts</option>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.layout.splitSeries">false</option>
      <option name="charting.legend.placement">none</option>
      <option name="charting.primaryAxisTitle.text"/>
      <option name="charting.secondaryAxisTitle.text"/>
    </chart>
  </row>
  <row>
    <table>
      <searchName>Connection State break down last 24 hours</searchName>
      <title>Connection State break down last 24 hours</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">false</option>
      <option name="drilldown">all</option>
    </table>
  </row>
</dashboard>
